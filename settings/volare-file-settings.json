        {
          "type": "providers/throttle",
          "options": {
            "rate": 500
          }
        },
        {
          "type": "providers/liner"
        },
        {
          "type": "providers/nmea0183-signalk",
          "optionMappings": [
            {
              "fromAppProperty": "selfId",
              "toOption": "selfId"
            },
            {
              "fromAppProperty": "selfType",
              "toOption": "selfType"
            }
          ]
        }
      ]
    }
  ]
